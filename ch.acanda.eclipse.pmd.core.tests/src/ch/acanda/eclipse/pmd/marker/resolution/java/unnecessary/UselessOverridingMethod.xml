<tests>
  <pmdReferenceId>rulesets/java/unnecessary.xml/UselessOverridingMethod</pmdReferenceId>
  <language>java 1.7</language>

  <test name="UselessOverridingMethod">
    <setup>
      <source>
public class UseIndexOfChar {
    public <marker>void foo(String bar) {
        super.foo(bar);
    }</marker>
}
      </source>
    </setup>
    <expected>
      <source>
public class UseIndexOfChar {
}
      </source>
      <image>QUICKFIX_REMOVE</image>
      <label>Remove method</label>
      <description>Removes the method.</description>
    </expected>
  </test>

</tests>